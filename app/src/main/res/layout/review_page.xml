<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btnReview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/etvReview"
        android:layout_alignParentTop="true"
        android:layout_marginTop="629dp"
        android:layout_marginEnd="108dp"
        android:text="Button"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="607dp" />

    <MultiAutoCompleteTextView
        android:id="@+id/etvReview"
        android:layout_width="319dp"
        android:layout_height="239dp"
        android:layout_alignEnd="@+id/tvBarnamereview"
        android:layout_alignParentTop="true"
        android:layout_marginTop="334dp"
        android:layout_marginEnd="-81dp"
        android:text="Type in your review"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="297dp" />

    <RatingBar
        android:id="@+id/reviewrating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/tvBarnamereview"
        android:layout_alignParentTop="true"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="-41dp"
        android:contentDescription="@string/app_name"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="200dp" />

    <TextView
        android:id="@+id/tvBarnamereview"
        android:layout_width="166dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="109dp"
        android:layout_marginTop="85dp"
        android:text="Mcgoverns"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="47dp" />
</RelativeLayout>